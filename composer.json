{
    "name": "fragososoftware/sped-sdk",
    "description": "SDK para integração com as APIs do SPED (Sistema Público de Escrituração Digital).",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "src/Core/",
            "Infrastructure\\": "src/Infrastructure/",
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Esdras Fragoso",
            "email": "esdrasfragoso@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
